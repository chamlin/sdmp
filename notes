then



> Insert modules

~/mlcp/bin/mlcp.sh import -host localhost -port 8000 -username admin -password admin -input_file_path ./modules -mode local -database sdmp-modules -output_uri_replace "/Users/chamlin/git/sdmp/modules,''"


