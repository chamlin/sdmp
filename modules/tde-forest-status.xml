<template xmlns="http://marklogic.com/xdmp/tde">
    <path-namespaces>
        <path-namespace>
            <prefix>fs</prefix>
            <namespace-uri>http://marklogic.com/xdmp/status/forest</namespace-uri>
        </path-namespace>
    </path-namespaces>
    <context>/fs:forest-status</context>
    <vars>
        <var>
            <name>F</name>
            <val>xs:string ('http://marklogic.com/sdmp/')</val>
        </var>
        <var>
            <name>FIRI</name>
            <val>sem:iri (fn:concat ('http://marklogic.com/sdmp/', '#', fs:forest-id))</val>
        </var>
    </vars>
    <triples>
        <triple>
            <subject>
                <val>$FIRI</val>
            </subject>
            <predicate>
                <val>sem:iri('a')</val>
            </predicate>
            <object>
                <val>sem:iri('http://marklogic.com/sdmp/forest')</val>
            </object>
        </triple>
        <triple>
            <subject>
                <val>$FIRI</val>
            </subject>
            <predicate>
                <val>sem:iri($F||'forest-name')</val>
            </predicate>
            <object>
                <val>xs:string(fs:forest-name)</val>
            </object>
        </triple>
    </triples>
    <templates>
        <template>
            <context>fs:stands/fs:stand</context>
            <vars>
                <var>
                    <name>SIRI</name>
                    <val>sem:iri (fn:concat ('http://marklogic.com/sdmp/stand', '#', fs:stand-id))</val>
                </var>
            </vars>
            <triples>
                <triple>
                    <subject>
                        <val>$SIRI</val>
                    </subject>
                    <predicate>
                        <val>sem:iri('a')</val>
                    </predicate>
                    <object>
                        <val>sem:iri('http://marklogic.com/sdmp/stand')</val>
                    </object>
                </triple>                
                <triple>
                    <subject>
                        <val>$SIRI</val>
                    </subject>
                    <predicate>
                        <val>sem:iri($F||'stand-id')</val>
                    </predicate>
                    <object>
                        <val>xs:string(fs:stand-id)</val>
                    </object>
                </triple>
                <triple>
                    <subject>
                        <val>$SIRI</val>
                    </subject>
                    <predicate>
                        <val>sem:iri($F||'path')</val>
                    </predicate>
                    <object>
                        <val>xs:string(fs:path)</val>
                    </object>
                </triple>
                <triple>
                    <subject>
                        <val>$SIRI</val>
                    </subject>
                    <predicate>
                        <val>sem:iri($F||'stand-kind')</val>
                    </predicate>
                    <object>
                        <val>xs:string(fs:stand-kind)</val>
                    </object>
                </triple>
            </triples>
        </template>
    </templates>
</template>
